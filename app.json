{
	"expo": {
		"name": "blcalistenia",
		"slug": "blcalistenia",
		"version": "1.0.0",
		"scheme": "com.blcalistenia",
		"orientation": "portrait",
		"icon": "./assets/icon.png",
		"userInterfaceStyle": "light",
		"newArchEnabled": true,
		"splash": {
			"image": "./assets/splash-icon.png",
			"resizeMode": "contain",
			"backgroundColor": "#ffffff"
		},
		"experiments": {
			"typedRoutes": true
		},
		"ios": {
			"supportsTablet": true
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/adaptive-icon.png",
				"backgroundColor": "#ffffff"
			},
			"edgeToEdgeEnabled": true
		},
		"web": {
			"favicon": "./assets/favicon.png",
			"bundler": "metro"
		},
		"plugins": [
			[
				"expo-router",
				{
					"root": "./src/app"
				}
			]
		]
	}
}
